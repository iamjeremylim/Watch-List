<h1 class="text-center">New Bookmark</h1>
<div>
  <%= simple_form_for @bookmark do |f| %>
    <%= f.input :comment %>
    <%= f.association :movie %>
    <%= f.button :submit, class: "btn btn-flat btn-block" %>
    <%= link_to "Back to list", lists_path, class: "btn btn-flat btn-block" %>
  <% end %>
</div>
